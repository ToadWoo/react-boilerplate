{
    "compilerOptions": {
      "outDir": "./dist/",
      "baseUrl": "./",
      "noImplicitAny": true,
      "module": "esnext",
      "target": "es5",
      "jsx": "react",
      "allowSyntheticDefaultImports":true,
      "esModuleInterop": true,
      "allowJs": true,
      "moduleResolution": "Node",
      "sourceMap": true,
      "paths": {
        "@/images/*": ["./src/images/*"],
        "@/components/*": ["./src/components/*"],
        "@/pages/*": ["./src/pages/*"]
      }
    },
    "include": ["src"]
  }